{
    "$schema": "vscode://schemas/settings/default",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontSize": 14,
    "gitlens.defaultDateShortFormat": null,
    "gitlens.defaultDateFormat": null,
    "python.defaultInterpreterPath": "C:\\Program Files\\Python313\\python.exe",
    "[c]": {
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true
    },
    "workbench.startupEditor": "none",
    "cmake.configureOnOpen": true,
    "git.autofetch": true,
    "workbench.editorAssociations": {
        "*.docx": "default"
    },
    "workbench.editor.empty.hint": "hidden",
    "cmake.options.statusBarVisibility": "visible",
    "[dart]": {
        "editor.rulers": [80],
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off",
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "window.confirmSaveUntitledWorkspace": false,
    "gitlens.rebaseEditor.ordering": "asc",
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "workbench.iconTheme": "material-icon-theme",
    "window.restoreWindows": "none",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "files.autoSave": "afterDelay",
    "explorer.confirmDelete": false,
    "window.titleBarStyle": "custom",
    "explorer.expandSingleFolderWorkspaces": false,
    "terminal.integrated.cursorBlinking": true,
    "editor.cursorBlinking": "blink",
    "editor.fontFamily": "Jetbrains Mono, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.smoothScrolling": true,
    "editor.mouseWheelZoom": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.wordWrap": "off",
    "editor.cursorSurroundingLines": 8,
    "editor.lineNumbers": "on",
    "github.copilot.nextEditSuggestions.enabled": false,
    "github.copilot.enable": {
        "*": false
    },
    "better-comments.tags": [
        {
            "tag": "TODO:",
            "color": "#EA580C",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "FIXME:",
            "color": "#FF2D00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "BUG:",
            "color": "#9333EA",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "HACK:",
            "color": "#C026D3",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "NOTE:",
            "color": "#2563EB",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "INFO:",
            "color": "#0EA5E9",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "IDEA:",
            "color": "#FFF",
            "strikethrough": false,
            "backgroundColor": "#EAB308"
        }
    ],
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "prettier.tabWidth": 4,
    "prettier.semi": true,
    "prettier.singleQuote": false,
    "prettier.useTabs": false,
    "workbench.colorTheme": "GitHub Dark Default",
    "remote.defaultExtensionsIfInstalledLocally": [
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "GitHub.vscode-pull-request-github"
    ],
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "files.associations": {
        "*.json": "json"
    },
    "git.confirmSync": false,
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.enableSmartCommit": true,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4o",
    "gitlens.ai.largePromptWarningThreshold": 50000,
    "editor.minimap.size": "fit",
    "editor.minimap.showSlider": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "chat.instructionsFilesLocations": {
        ".github/instructions": true,
        "/tmp/postman-http-request-post-response.instructions.md": true,
        "/tmp/postman-http-request-pre-request.instructions.md": true,
        "/tmp/postman-collections-post-response.instructions.md": true,
        "/tmp/postman-collections-pre-request.instructions.md": true,
        "/tmp/postman-folder-post-response.instructions.md": true,
        "/tmp/postman-folder-pre-request.instructions.md": true
    },
    "go.toolsManagement.autoUpdate": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "postman.mcp.notifications.postmanMCP": false,
    "typescript.tsserver.maxTsServerMemory": 2048,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.compositeKeys": {}
}
